#include "libft_bonus.h"
#include <stdlib.h>

t_list	*ft_lstmap(t_list *lst, void *(*f)(void *), void (*del)(void *))
{
	t_list	*new_list;
	t_list	*head;
	t_list	*tail;

	if (!lst || !f || !del)
		return (NULL);
	head = lst;
	while (lst)
	{
		head->content = f(lst->content);
		if (!head)
		{
			ft_lst
			return (NULL);
		}
	}
}
